<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AcademicoActivity">

    <ImageView
        android:id="@+id/academico_logo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/academico_logo"
        tools:layout_editor_absoluteX="5dp"
        tools:layout_editor_absoluteY="72dp" />

    <TextView
        android:id="@+id/academico_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Servicio encargado de la administración acádemica de los estudiantes de ESPOL."
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="503dp" />
</androidx.constraintlayout.widget.ConstraintLayout>